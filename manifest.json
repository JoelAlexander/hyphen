{
  "name": "hyphen",
  "resources": {
    "houseWallet": {
      "type": "PrivateKey",
      "options": {
        "random": true
      }
    }
  },
  "namespaces": {
    "eth": {
      "type": "FIFSRegistrar"
    }
  },
  "deploy": {
    "faucetauthorizer": {
      "source": "@local-blockchain-toolbox/contract-primitives/SingleAccountAuthorizer"
    },
    "faucet": {
      "source": "@local-blockchain-toolbox/contract-primitives/AuthorizedFaucet",
      "args": [ "{faucetauthorizer}" ]
    },
    "recipehub": {
      "source": "RecipeHub"
    },
    "recipes": {
      "source": "RecipeSet"
    },
    "measures": {
      "source": "@local-blockchain-toolbox/contract-primitives/StringSet"
    },
    "curation": {
      "source": "Curation"
    }
  }
}
